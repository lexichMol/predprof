    def go(self):
        if self.radioButton.isChecked() == self.radioButton_2.isChecked():
            error = QMessageBox()
            error.setWindowTitle("ошибка")
            error.setText("выберите количество гонщиков")
            error.setIcon((QMessageBox.Warning))
            error.setStandardButtons(QMessageBox.Ok)
            error.exec_()
        if self.radioButton.isChecked() == True:
            Window.setWindowTitle(self.type_z.currentText())
            Window.show()
            MainWindow.hide()
        elif self.radioButton_2.isChecked() == True:
            Window.setWindowTitle(self.type_z.currentText())
            i.label.setText("номер первого гонщика")
            i.label.setGeometry(QtCore.QRect(10, 10, 120, 20))
            i.lineEdit.setGeometry(QtCore.QRect(135, 10, 100, 20))


            i.label_2 = QtWidgets.QLabel(i.centralwidget)
            i.label_2 .setGeometry(QtCore.QRect(10, 30, 120, 20))
            i.label_2 .setObjectName("label_2")
            i.label_2.setText("номер второго гонщика")
            i.label_2.show()

            i.lineEdit_2 = QtWidgets.QLineEdit(i.centralwidget)
            i.lineEdit_2.setGeometry(QtCore.QRect(135, 30, 100, 20))
            i.lineEdit_2.setText("")
            i.lineEdit_2.setObjectName("lineEdit_2")

            Window.show()
            MainWindow.hide()


